<%= render 'shared/error' %>
<%= simple_form_for @user do |f| %>
    <%= f.input :username%>

    <%= f.input :email %> 

      <%= f.simple_fields_for :socials, {class: 'inline-block m-3'} do | b |  %>
              <%= b.input :name, wrapper: :dynamic %>
      <% end %>
  <%= f.button :submit %>
<% end %>

<%= link_to 'Return to list of users', users_path, method: :get %>
