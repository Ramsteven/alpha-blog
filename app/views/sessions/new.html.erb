<div class="form-inputs m-4">
<%= form_with(scope: :session, url: login_path) do |f| %>
  <div class="form-inputs m-4">

    <div class="m-2">
      <%= f.label :email%>
      <%= f.text_field :email, :as => :string, placeholder: "Enter your email"  %>
    </div>

    <div class="m-2">
      <%= f.label :password%>
      <%= f.text_field :password, :as => :string, placeholder: "Enter your password" %>
    </div>

        <%= f.submit "Log In" , class: 'btn btn-success' %>
    </div>
<% end%>

