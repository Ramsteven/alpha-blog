<%= render 'shared/error' %>
<%= form_for @user do |f| %>
    <%= f.label :username%>
    <%= f.text_field :username%>

    <%= f.label :email %> 
    <%= f.text_field :username%>

    <%= f.fields_for :socials, {class: 'inline-block m-3'} do | b |  %>

        <%= b.label :name %>
        <%= b.text_field :name %>

        <%= b.label :link %>
        <%= b.text_field :link %>

      <% end %>
  <%= f.submit %>
<% end %>

<%= link_to 'Return to list of users', users_path, method: :get %>
